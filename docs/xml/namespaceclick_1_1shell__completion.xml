      <memberdef kind="variable" id="namespaceclick_1_1shell__completion_1af0454934aead57f6e51a31cbb4d0f971" prot="protected" static="no" mutable="no">
        <type>str</type>
        <definition>str click::shell_completion._SOURCE_FISH</definition>
        <argsstring></argsstring>
        <name>_SOURCE_FISH</name>
        <qualifiedname>click.shell_completion._SOURCE_FISH</qualifiedname>
        <initializer>=  &quot;&quot;&quot;\
function %(complete_func)s;
    set -l response (env %(complete_var)s=fish_complete COMP_WORDS=(commandline -cp) \
COMP_CWORD=(commandline -t) %(prog_name)s);

    for completion in $response;
        set -l metadata (string split &quot;,&quot; $completion);

        if test $metadata[1] = &quot;dir&quot;;
            __fish_complete_directories $metadata[2];
        else if test $metadata[1] = &quot;file&quot;;
            __fish_complete_path $metadata[2];
        else if test $metadata[1] = &quot;plain&quot;;
            echo $metadata[2];
        end;
    end;
end;

complete --no-files --command %(prog_name)s --arguments \
&quot;(%(complete_func)s)&quot;;
&quot;&quot;&quot;</initializer>
